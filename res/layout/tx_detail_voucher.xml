<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data class="VoucherDetailBinding" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorPrimary">

        <RelativeLayout
            android:id="@+id/secHeader"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#ffffff"          >

            <ImageButton
                android:id="@+id/btn_back"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:padding="10dp"
                android:layout_alignParentLeft="true"
                android:adjustViewBounds="true"
                android:scaleType="centerInside"
                android:background="@null"
                app:srcCompat="@drawable/ylp_ico_back" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:inputType="number"
                android:text="Voucher"
                android:singleLine="true"
                android:textAlignment="center"
                android:textSize="10pt"
                android:fontFamily="@font/akzidenzgrotesk_light"
                android:textColor="@color/grgray"/>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/secHeader"
            android:background="@null"
            android:orientation="vertical">
            <com.wizarpos.emvsample.activity.HeaderTicketView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="17dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="10dp"
                android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/img_brand_card"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:adjustViewBounds="true"
                        android:scaleType="centerInside"
                        android:layout_margin="5dp"
                        android:src="@drawable/ylp_ico_master"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/monto"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="2dp"
                            android:autoSizeMinTextSize="1dp"
                            android:text="$0.0"
                            android:textAlignment="textEnd"
                            android:textColor="@color/greenYLP"
                            android:fontFamily="@font/akzidenzgrotesk_regular"
                            android:textSize="25dp"/>
                        <TextView
                            android:id="@+id/status"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="2dp"
                            android:autoSizeMinTextSize="1dp"
                            android:text="Aprobado"
                            android:textAlignment="textEnd"
                            android:textColor="@color/darkGrayYLP"
                            android:fontFamily="@font/akzidenzgrotesk_regular"
                            android:textSize="12dp"/>
                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:id="@+id/fecha_hora"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:autoSizeMinTextSize="1dp"
                    android:text="15/05/2020 15:30"
                    android:textAlignment="textEnd"
                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="13dp"/>
                <TextView
                    android:id="@+id/tarjeta"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:autoSizeMinTextSize="1dp"
                    android:text="Banorte Credito ******8456"
                    android:textAlignment="textEnd"
                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="13dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Folio:"
                    android:singleLine="true"
                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="14dp"/>
                <TextView
                    android:id="@+id/folio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Folio"
                    android:singleLine="true"
                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Comercio:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="14dp"/>
                <TextView
                    android:id="@+id/comercio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Comercio"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Afiliación:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="14dp"/>
                <TextView
                    android:id="@+id/afiliacion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Folio"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Terminal:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="14dp"/>
                <TextView
                    android:id="@+id/terminal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Terminal"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Número control:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="14dp"/>
                <TextView
                    android:id="@+id/numero_control"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Numero Control"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Código Autorización:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="14dp"/>
                <TextView
                    android:id="@+id/codigo_autorizacion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="codigo autorizacion"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Referencia:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/referencia"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Referencia"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="15dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/field_referencia_original"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Referencia Origen:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/referencia_original"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="Referencia"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="15dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text=""
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="5dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="AID:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="13dp"/>
                <TextView
                    android:id="@+id/aid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="000"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="13dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="TVR:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="13dp"/>
                <TextView
                    android:id="@+id/tvr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="000"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="13dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="TSI:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="13dp"/>
                <TextView
                    android:id="@+id/tsi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="000"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="13dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="APN:"
                    android:singleLine="true"

                    android:textColor="@color/darkGrayYLP"
                    android:fontFamily="@font/akzidenzgrotesk_regular"
                    android:textSize="13dp"/>
                <TextView
                    android:id="@+id/apn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="textStart"
                    android:inputType="number"
                    android:text="000"
                    android:singleLine="true"

                    android:textColor="@color/grgray"
                    android:fontFamily="@font/akzidenzgrotesk_light"
                    android:textSize="13dp"/>
            </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:gravity="right"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="18dp"
                android:layout_marginLeft="18dp">

                <ImageButton
                    android:id="@+id/btn_cancelar"
                    android:layout_width="32dp"
                    android:layout_height="32dp"

                    android:adjustViewBounds="true"
                    android:background="@null"
                    android:scaleType="centerInside"
                    android:src="@drawable/ylp_ico_cancel" />

                <ImageButton
                    android:id="@+id/btn_reimprimir"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:adjustViewBounds="true"
                    android:scaleType="centerInside"
                    android:background="@null"
                    android:src="@drawable/ylp_ico_print"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    />
                <ImageButton
                    android:id="@+id/btn_ticket"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:adjustViewBounds="true"
                    android:scaleType="centerInside"
                    android:background="@null"
                    android:layout_above="@+id/reimprimir"
                    android:src="@drawable/ylp_ico_ticket"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="60dp"
                    />
            </RelativeLayout>
        </RelativeLayout>

    </RelativeLayout>
</layout>