<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="CustomWindowTitleBackground">
        <item name="android:background">#DEDEDE</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MyMenu">
        <item name="android:textSize">0dp</item>
    </style>

    <style name="btnButtonProductos">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/blueYLP</item>
        <item name="android:fontFamily">@font/akzidenzgrotesk_light</item>
    </style>
    <style name="btnButtonProductosGreen">
        <item name="android:textSize">10pt</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:fontFamily">@font/akzidenzgrotesk_light</item>
    </style>

    <style name="NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />


    <style name="PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="whitePurpleStyle" parent="@android:style/Widget.Button">
        <item name="android:textSize" >30dp</item>
        <item name="android:textColor">@color/blueYLP</item>
        <item name="android:background">@drawable/white_bg_button_prod</item>
    </style>
</resources>