            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.ShowLastPBOCActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.ShowTransInfoActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.TransResultActivity"
            android:screenOrientation="nosensor" />
        <activity android:name=".activity.Sale" />
        <activity android:name=".activity.Settle" />
        <activity android:name=".activity.QueryCardRecord" />
