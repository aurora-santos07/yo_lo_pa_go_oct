            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.InputAmountActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.InputPINActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.InputOfflinePINActivity"
            android:windowSoftInputMode="adjustNothing"
            android:screenOrientation="nosensor"  />
        <activity
            android:name=".activity.ProcessEMVCardActivity"
