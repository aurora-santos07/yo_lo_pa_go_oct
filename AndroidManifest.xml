            android:screenOrientation="nosensor"
            android:windowSoftInputMode="adjustNothing"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.IdleActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.FuncMenuActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.RequestCardActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.RemoveCardActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.ConfirmBypassPinActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.ConfirmCardActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.DataClearActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.EnquireTransActivity"
            android:screenOrientation="nosensor" />
        <activity
